
cellranger count --id=LD5RC --transcriptome=refdata-cellranger-mm10-3.0.0 --fastqs=~/MGseq201957/LD_AVM02_FastQ/LD5RC --sample=LD5RC --chemistry=SC3Pv3 --nosecondary && echo "B1_yes_cellranger3.1.0" >> didIwork.txt
cp -rf ~/../ubuntusc/MGseq201957/LD_AVM02_data_outs/LD5RC ~/../../media/sf_SharedFolder
rm -rf ~/../ubuntusc/MGseq201957/LD_AVM02_data_outs/LD5RC

